# platform :ios, '9.0'

target 'kitsune' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for kitsune
  pod 'RealmSwift'

  target 'kitsuneTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RealmSwift'
  end

  target 'kitsuneUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RealmSwift'
  end

end
